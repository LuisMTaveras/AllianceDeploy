<?xml version="1.0" encoding="utf-8" ?>
    <?xaml-comp compile="true"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <!--<Setter Property="" Value="" />-->

    <!--#region LabelTitulo -->
    <Style Class="LabelTitulo" TargetType="Label">
        <Setter Property="FontAttributes" Value="Bold"/>
        <Setter Property="Padding" Value="5"/>
        <Setter Property="FontSize" Value="Title" />
        <Setter Property="TextColor" Value="{AppThemeBinding Dark={StaticResource LightText}, Light={StaticResource DarkText}}" />
        <Setter Property="HorizontalOptions" Value="Start" />
    </Style>
    <!--#endregion-->

    <!--#region Frame -->
    <Style TargetType="Frame" Class="FramePrincipal">
        <Setter Property="HasShadow" Value="False"/>
        <Setter Property="CornerRadius" Value="20"/>
        <Setter Property="Margin" Value="5,10,5,5"/>
        <Setter Property="Padding" Value="15"/>
        <Setter Property="BackgroundColor" Value="{AppThemeBinding Dark={StaticResource DarkFrame}, Light={StaticResource LightFrame}}" />
    </Style>
    <!--#endregion-->

    <!--#region Theme -->
        
    <!--#endregion-->


    <!--#region Button -->
        
    <!--#endregion-->

</ResourceDictionary>